{
  "name": "manishsfantasticdump",
  "displayName": "Manish's Fantastic Dump",
  "publisher": "ManishVenumuddula",
  "description": "Run 'ncdump -h' on '.nc' files for accessibility in VSCode.",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.50.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manishvenu/manishs-fantastic-dump"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "netcdf.viewer",
        "displayName": "NetCDF Viewer",
        "selector": [
          {
            "filenamePattern": "*.nc"
          }
        ]
      }
    ]
  },
  "activationEvents": [
    "onFileSystem:.nc"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.50.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}